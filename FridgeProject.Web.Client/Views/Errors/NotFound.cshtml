@{
    ViewData["Title"] = "Error!";
}

<div class="text-center">
    <h1 class="display-4">Warning! Error! </h1>
    <h2 class="mt-2">CODE:404 Not Found.</h2>
    <h3 class="mt-3">There is no resource for your request! </h3>
    <h3 class="mt-3"> Please check data or call your Administrator!</h3>
   


    <h3 class="mt-5">To go back just click "Go Back". </h3>
    <div class="mt-2 row form-group">
        <button onclick="history.back()" class=" btn-outline-primary mr-3">Go Back</button>
    </div>
</div>
